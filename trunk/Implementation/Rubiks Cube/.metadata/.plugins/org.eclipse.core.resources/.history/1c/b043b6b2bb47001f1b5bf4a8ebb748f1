package cube;

public class PrimaryFace extends Face {

	public PrimaryFace(CornerCubicle corner1, CornerCubicle corner2,
			CornerCubicle corner3, CornerCubicle corner4, EdgeCubicle edge1,
			EdgeCubicle edge2, EdgeCubicle edge3, EdgeCubicle edge4,
			Facelet faceColor) {
		super(corner1, corner2, corner3, corner4, edge1, edge2, edge3, edge4, faceColor);
	}
	
	public PrimaryFace(CornerCubicle[] cornerArray, EdgeCubicle[] edgeArray,
			Facelet faceColor) {
		super(cornerArray, edgeArray, faceColor);
	}


}
